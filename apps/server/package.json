{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Tango server (Express + TypeScript)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "dotenv -e ./.env --overload -- tsx --tsconfig tsconfig.json src/index.ts",
    "dev:host": "cross-env REDIS_URL=redis://localhost:6379 DATABASE_URL=postgres://postgres:postgres@localhost:5432/tango dotenv -e ./.env -- tsx --tsconfig tsconfig.json src/index.ts",
    "dev:watch": "dotenv -e ./.env --overload -- tsx watch --tsconfig tsconfig.json src/index.ts",
    "build": "dotenv -e ./.env --overload -- tsc",
    "start": "dotenv -e ./.env --overload -- node dist/index.js",
    "start:prod": "dotenv -e ./.env.prod -- node dist/index.js",
    "start:staging": "dotenv -e ./.env.staging -- node dist/index.js",
    "build:ci": "tsc",
    "test:security": "node scripts/test-security.js",
    "test:security:prod": "NODE_ENV=production node scripts/test-security.js",
    "test:metrics": "node scripts/test-metrics.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "jest --runInBand --passWithNoTests",
    "format:check": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rionegro8612-cyber/tangos.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rionegro8612-cyber/tangos/issues"
  },
  "homepage": "https://github.com/rionegro8612-cyber/tangos#readme",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@types/node-cron": "^3.0.11",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "pino-loki": "^2.6.0",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.3",
    "redis": "^4.7.1",
    "twilio": "^5.8.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.1",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  }
}
